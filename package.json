{
  "name": "arocord-hims",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^2.1.10",
    "@reduxjs/toolkit": "^2.3.0",
    "@sentry/react": "^10.32.1",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-query": "^5.62.7",
    "@types/dompurify": "^3.0.5",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.468.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.1.0",
    "recharts": "^2.13.3",
    "sonner": "^2.0.7",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.11.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "happy-dom": "^20.0.11",
    "jsdom": "^25.0.1",
    "msw": "^2.12.4",
    "playwright-lighthouse": "^4.0.0",
    "postcss": "^8.5.1",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "deploy:staging": "npm run build:staging && aws s3 sync dist/ s3://hims-staging-frontend --delete",
    "deploy:production": "npm run build:production && echo 'Use GitHub Actions for production deployment'",
    "test:a11y": "playwright test e2e/tests/accessibility.spec.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit --project='Microsoft Edge' --project='Google Chrome'",
    "test:devices": "playwright test --project='Mobile Chrome' --project='Mobile Safari' --project=Tablet --project='Large Desktop'",
    "test:runner": "node scripts/test-runner.js",
    "test:runner:cross-browser": "node scripts/test-runner.js cross-browser",
    "test:runner:devices": "node scripts/test-runner.js devices",
    "test:runner:accessibility": "node scripts/test-runner.js accessibility",
    "test:runner:performance": "node scripts/test-runner.js performance",
    "test:runner:role-based": "node scripts/test-runner.js role-based",
    "test:runner:all": "node scripts/test-runner.js all",
    "test:report": "node scripts/test-reporter.js",
    "test:dashboard": "npm run test:runner:all && npm run test:report && open test-report/index.html"
  }
}
