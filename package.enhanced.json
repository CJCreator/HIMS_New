{
  "name": "arocord-hims",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.9.7",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "recharts": "^3.6.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-a11y": "^7.6.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "happy-dom": "^20.0.11",
    "jest-axe": "^8.0.0",
    "jsdom": "^27.3.0",
    "msw": "^2.0.11",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "storybook": "^7.6.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run src/**/*.{test,spec}.{ts,tsx}",
    "test:integration": "vitest run src/**/*integration*.{test,spec}.{ts,tsx}",
    
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    
    "test:a11y": "vitest run src/**/*a11y*.{test,spec}.{ts,tsx}",
    "test:performance": "vitest run src/**/*performance*.{test,spec}.{ts,tsx}",
    
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=<your-project-token>",
    
    "install:playwright": "playwright install",
    "install:test-deps": "npm install && playwright install"
  },
  "msw": {
    "workerDirectory": "public"
  }
}