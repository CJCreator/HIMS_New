# Staging Environment Configuration
# API Configuration
VITE_API_URL=https://api-staging.hims.arocord.com
VITE_WS_URL=wss://ws-staging.hims.arocord.com

# Environment
VITE_ENVIRONMENT=staging

# Monitoring & Analytics
VITE_SENTRY_DSN=your-staging-sentry-dsn
VITE_ANALYTICS_ID=your-staging-analytics-id

# Feature Flags
VITE_FEATURE_TELEMEDICINE=true
VITE_FEATURE_AI_DIAGNOSIS=false
