# Security
VITE_CSP_REPORT_URI=https://api-staging.hims.arocord.com/csp-report

# Performance
VITE_PERFORMANCE_MONITORING=true
VITE_WEB_VITALS_ENDPOINT=https://api-staging.hims.arocord.com/web-vitals

# Debug
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug